# Database Configuration

DB_HOST=your-db-host
DB_PORT=5432
DB_NAME=your-db-name
DB_USER=your-db-user
DB_PASSWORD=your-db-password

# API Keys (optional - most APIs don't require keys)
# UNICEF_API_KEY=your_unicef_api_key_if_needed
# OECD_API_KEY=your_oecd_api_key_if_needed

# Logging
LOG_LEVEL=INFO
LOG_FILE=etl_logs/etl_process.log

# ETL Configuration
ETL_START_YEAR=2010
ETL_CACHE_DIR=data_cache

# Countries to fetch (comma-separated list of ISO3 codes)
COUNTRIES=USA,GBR,DEU,JPN,CHN,FRA,IND,AUS,BRA,CAN,RUS,ZAF,KOR,TUR,ITA,ESP,NLD,BEL,CHE,SWE

# Retry configuration
MAX_RETRIES=3
RETRY_DELAY=2

# Rate limiting
API_DELAY=0.5